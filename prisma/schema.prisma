datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Booking {
  id             Int      @id @default(autoincrement())
  date           DateTime
  time           String
  name           String
  contact        String
  pickUpAddress  String
  dropOffAddress String
  driver         String
  comments       String?
  agent          String
  voucherNumber  String
}
